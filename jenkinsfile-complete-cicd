@Library('cicd-project-lib') _
pipeline{
    agent any
    stages{
        stage('git-Checkout'){
            steps{
                script{
                  def config = [
                    Url:'https://github.com/Kavana1997/itd_python_flask_app.git',
                    CredID: 'github-cred',
                    Branch: 'main'
                  ]
                  gitCheckout(config)
                }
            }
        }

        stage('sonar-scan'){
          steps{
            script{
              sh '''
              which sonar-scanner
              sonar-scanner -V
              '''
            }
          }
        }
    }
}
