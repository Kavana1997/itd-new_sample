@Library('cicd-project-lib') _
pipeline{
    agent any
    stages{
        stage('git-Checkout'){
            steps{
                def config = [
                  url:'https://github.com/Kavana1997/itd_python_flask_app.git',
                  credentialsID: 'github-cred',
                  name: 'main'
                ]
                gitCheckout(config)
            }
        }
    }
}